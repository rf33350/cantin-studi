---
title: Contact
layout: layout.njk
---

<div class="text-center">
    <h1>{{title}}</h1>
</div>

<form>
  <div class="form-group mx-4 mb-3">
    <label for="exampleInputEmail1">Email</label>
    <input type="email" class="form-control" id="eemail" aria-describedby="emailHelp" placeholder="Entrez votre email">
  </div>
  <div class="form-group mx-4 mb-3">
    <label for="message">Message</label>
    <textarea class="form-control" id="message" rows="3"></textarea>
  </div>
  <button type="submit" class="btn btn-secondary mx-4 ">Envoyer</button>
</form>